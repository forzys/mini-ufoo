video {
  width: 100%;
  height: 100%;
  margin: 0; 
  display: block; 
}

.page {
  width: 100%;
  height: 100%;
}

::-webkit-scrollbar {
  display: none;
  width: 0;
  height: 0;
  color: transparent;
}
