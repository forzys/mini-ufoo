.wall-paper,
.paper-col {
  width: 100%;
  height: 100%;
}
.paper-row {
  min-height: 10vh;
  margin: 20rpx;
  overflow: hidden;
  text-align: center;
}
.paper-row .paper-col:first-child {
  margin-right: 10rpx;
}
.paper-row .paper-col:last-child {
  margin-left: 10rpx;
}
.paper-img {
  width: 100%;
  height: 100%;
  border-radius: 8px;
  box-shadow: 0 1rpx 6rpx rgba(0, 0, 0, 0.1);
}
